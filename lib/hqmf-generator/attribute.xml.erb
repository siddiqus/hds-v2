	<subjectOf>
		<measureAttribute>
			<id root="2.16.840.1.113883.3.100.1" extension="<%= attribute.id %>"/>
			<code code="<%= attribute.code %>">
				<displayName value="<%= attribute.name %>"/>
			</code>
			<%- if attribute.value -%>
			<value xsi:type="ED" value="<%= attribute.value %>"/>
			<%- end -%>
		</measureAttribute>
	</subjectOf>
